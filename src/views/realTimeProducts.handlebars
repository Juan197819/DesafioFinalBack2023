{{>header}}
<div class="flexCol">
    <h1>Carga de articulos</h1>
    <form class="form" id="form">
        <label for="title">Nombre del producto</label>
        <input id='title' name="title" type="text" value="arroz">
        <label for="description">Descripcion</label>
        <input id='description' name="description" type="text" value="triple cero">
        <label for="stock">Cantidad en Stock</label>
        <input id='stock' name="stock" type="number" value=12>
        <label for="category">Categoria de producto</label>
        <input id='category' name="category" type="text" value='almacen'>
        <label for="code">Codigo</label>
        <input id='code' name="code" type="text" value="asd131">
        <label for="price">Precio</label>
        <input id='price' name="price" type="number" value=350>
        <label for="thumbnail">Imagen</label>
        <input id='thumbnail' name="thumbnail" type="text" value="asdada/fdgdfg/dfd.png">
        <input type="submit">
    </form>
</div>
<h2>Productos</h2>
<table class="flexCol">
    <thead>
        <tr>
            <th>Producto</th>
            <th>Descripcion</th>
            <th>Precio</th>
            <th>Stock</th>
            <th>Categoria</th>
        </tr>
    </thead>
    <tbody id='tbody'>
    </tbody >
</table>
